{
  "config": {
    "target": "android",
    "proxy": {
      "ip": "127.0.0.1",
      "port": "8080"
    },
    "android": {
      "template": "android-30.tmpl"
    },
    "ios": {}
  },
  "name": "appium-runner",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "android": "make run-android ANDROID_TEMPLATE=$npm_package_config_android_template PROXY=$npm_package_config_proxy_ip:$npm_package_config_proxy_port &",
    "gen-cert": "node ./src/gen-cert.js",
    "inject-cert": "make android-inject-cert ANDROID_TEMPLATE=$npm_package_config_android_template PROXY=$npm_package_config_proxy_ip:$npm_package_config_proxy_port",
    "test:android": "./node_modules/.bin/mocha --timeout 100000 --config .mocharc.js 'src/__test__/segment-data-android.test' && adb emu kill",
    "test:ios": "./node_modules/.bin/mocha --timeout 100000 --config .mocharc.js 'src/__test__/segment-data-ios.test.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "appium": "^1.22.3",
    "chai": "^4.3.4",
    "http-mitm-proxy": "^0.9.0",
    "mitmproxy": "^2.1.2",
    "mocha": "^9.0.3",
    "webdriverio": "^7.9.0"
  },
  "devDependencies": {
    "mochawesome": "^7.1.3"
  }
}
